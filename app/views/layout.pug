doctype html
html(class="no-js" lang="pt-br")
    head(id="document-head")
        block head
        meta(charset="utf-8")
        meta(http-equiv="x-ua-compatible" content="ie=edge")
        title="Menu - A Universal APP"
        link(href='https://fonts.googleapis.com/css?family=Product+Sans:400,400i,700,700i' rel='stylesheet' type='text/css')
        link(rel='stylesheet' href="/css/font-awesome.css")
        link(rel='stylesheet' href="/css/fa-solid.css")
        link(rel='stylesheet' href='/css/icon.css')
        link(rel='stylesheet' href='/css/jquery-ui.css')
        style.
            html,
            body {
                overflow: hidden;
                width: 100%;
                height: 100%;
                margin: 0;
                padding: 0;
                font-family: Whitney, Helvetica Neue, Helvetica, Arial, sans-serif;
            }
            .loading-init {
                position: absolute;
                top: 0;
                bottom: 0;
                left: 0;
                right: 0;
                display: flex;
            }
            .loading-init .bg--effects {
                position: absolute;
                width: 100%;
                height: 100%;
                background-color: #DBC392;
                z-index: 10;
            }
            .loading-init .typing-3eiiL_ {
                display: -webkit-box;
                display: -ms-flexbox;
                display: flex;
                font-size: 63px;
                font-weight: 400;
                margin: auto;
                z-index: 140;
            }
            .loading-init .typing-3eiiL_ .ellipsis--nKTEd {
                margin: auto;
            }
            .loading-init .typing-3eiiL_ .ellipsis--nKTEd .spinner-pulsing-ellipsis {
                display: -webkit-box;
                display: -ms-flexbox;
                display: flex;
            }
            .loading-init .typing-3eiiL_ .ellipsis--nKTEd .spinner-pulsing-ellipsis .spinner-item {
                -webkit-animation: spinner-ellipsis 1.4s infinite ease-in-out;
                animation: spinner-ellipsis 1.4s infinite ease-in-out;
                background-color: #d7dadc;
                border-radius: 50px;
                display: inline-block;
                height: 20px;
                margin-right: 4px;
                opacity: .3;
                width: 20px;
            }
            .loading-init .typing-3eiiL_ .ellipsis--nKTEd .spinner-pulsing-ellipsis .spinner-item:nth-of-type(2) {
                -webkit-animation-delay: .2s;
                animation-delay: 0.2s;
            }
            .loading-init .typing-3eiiL_ .ellipsis--nKTEd .spinner-pulsing-ellipsis .spinner-item:nth-of-type(3) {
                -webkit-animation-delay: .4s;
                animation-delay: 0.4s;
            }
            @-webkit-keyframes spinner-ellipsis {
                0% {
                    -webkit-transform: scale(1);
                    opacity: 1;
                    transform: scale(1);
                }
                50% {
                    -webkit-transform: scale(0.8);
                    opacity: .3;
                    transform: scale(0.8);
                }
                to {
                    -webkit-transform: scale(1);
                    opacity: 1;
                    transform: scale(1);
                }
            }
            @-moz-keyframes spinner-ellipsis {
                0% {
                    -webkit-transform: scale(1);
                    opacity: 1;
                    transform: scale(1);
                }
                50% {
                    -webkit-transform: scale(0.8);
                    opacity: .3;
                    transform: scale(0.8);
                }
                to {
                    -webkit-transform: scale(1);
                    opacity: 1;
                    transform: scale(1);
                }
            }
            @-ms-keyframes spinner-ellipsis {
                0% {
                    -webkit-transform: scale(1);
                    opacity: 1;
                    transform: scale(1);
                }
                50% {
                    -webkit-transform: scale(0.8);
                    opacity: .3;
                    transform: scale(0.8);
                }
                to {
                    -webkit-transform: scale(1);
                    opacity: 1;
                    transform: scale(1);
                }
            }
            @-o-keyframes spinner-ellipsis {
                0% {
                    -webkit-transform: scale(1);
                    opacity: 1;
                    transform: scale(1);
                }
                50% {
                    -webkit-transform: scale(0.8);
                    opacity: .3;
                    transform: scale(0.8);
                }
                to {
                    -webkit-transform: scale(1);
                    opacity: 1;
                    transform: scale(1);
                }
            }
            @keyframes spinner-ellipsis {
                0% {
                    -webkit-transform: scale(1);
                    opacity: 1;
                    transform: scale(1);
                }
                50% {
                    -webkit-transform: scale(0.8);
                    opacity: .3;
                    transform: scale(0.8);
                }
                to {
                    -webkit-transform: scale(1);
                    opacity: 1;
                    transform: scale(1);
                }
            }

    body(id="document-body")
        div.app-mount
            div
                div.titlebar
                    div.titlebar-edge
                    block titlebar
                div
                    .app.animation-default
                        block app
                    .blur.animation-default(style="background: url('') round; filter: blur(0px); z-index: 100;")
                block content
                .downmost
                    block downmost
                .loading-init
                    .bg--effects
                    .typing-3eiiL_
                        span.ellipsis--nKTEd
                            span.spinner-inner.spinner-pulsing-ellipsis
                                span.spinner-item
                                span.spinner-item
                                span.spinner-item

        script(src='/js/jquery-3.2.1.js')
        script(src='/js/jquery-3.2.1.min.js')
        script(src='/js/jquery-ui.js')
        script(src='/js/material.min.js')
        script(src='/js/less.js')
        script(src='https://embed.twitch.tv/embed/v1.js')
        block scripts