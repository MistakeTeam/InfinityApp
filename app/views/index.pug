        button.win-minimize.animation-default
            svg(name="TitleBarMinimize" width="12" height="12" viewBox="0 0 12 12")
                rect(fill="#ffffff" width="10" height="1" x="1" y="6")
        button.win-maximize.animation-default
            svg(name="TitleBarMaximize" width="12" height="12" viewBox="0 0 12 12")
                rect(width="9" height="9" x="1.5" y="1.5" fill="none" stroke="#ffffff")
        button.win-close.animation-default
            svg(name="TitleBarClose" width="12" height="12" viewBox="0 0 12 12")
