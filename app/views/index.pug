extends layout

block head
    link(rel="stylesheet/less" type="text/css" href='/css/app.less')
    link(rel="stylesheet" href='/css/theme/lesser-dark.css')

block titlebar
    div.titlebar-buttons
        div(style="opacity: 0; display: block;").back-o4f98s.animation-default
            span.fa.fa-arrow-left
    div.win-buttons.win-buttons-light
        button.win-minimize.animation-default
            svg(name="TitleBarMinimize" width="12" height="12" viewBox="0 0 12 12")
                rect(fill="#ffffff" width="10" height="1" x="1" y="6")
        button.win-maximize.animation-default
            svg(name="TitleBarMaximize" width="12" height="12" viewBox="0 0 12 12")
                rect(width="9" height="9" x="1.5" y="1.5" fill="none" stroke="#ffffff")
        button.win-close.animation-default
            svg(name="TitleBarClose" width="12" height="12" viewBox="0 0 12 12")
                polygon(fill="#ffffff" fill-rule="evenodd" points="11 1.576 6.583 6 11 10.424 10.424 11 6 6.583 1.576 11 1 10.424 5.417 6 1 1.576 1.576 1 6 5.417 10.424 1")

block app
    div
        .inicio
            .invoq-menu
                if profile_data
                    .menu-topbar
                        .profile-18y63j(style='visibility: visible;')
                            .info-e5T5U3
                                span.username-2eY8s9 #{profile_data.username}
                            .avatar-3erY86
                                div(style=`height: 100%; background: url('${profile_data.avatarURL}') round;`)
                .menu-itens.animation-default(style="transform: scale(1, 1); opacity: 1; z-index: 100;")
                    .menu-contens
                        .contain-itens
                            .itens-fixed
                            div(style="display: none;").list-rest-itens
                                .loading-icon
                                    .typing-3eiiL_
                                        span.ellipsis--nKTEd
                                            span.spinner-inner.spinner-pulsing-ellipsis
                                                span.spinner-item
                                                span.spinner-item
                                                span.spinner-item
                .statusbar
                .util-app.animation-default(style="transform: scale(1.1, 1.1); opacity: 0; z-index: -1;")

