{
    "git.enableSmartCommit": true,
    "window.zoomLevel": -2,
    "typescript.check.npmIsInstalled": false,
    "git.confirmSync": false,
    "csharp.format.enable": false,
    "editor.accessibilitySupport": "off",
    "workbench.activityBar.visible": true,
    "workbench.statusBar.visible": true,
    "workbench.sideBar.location": "left",
    "editor.minimap.enabled": false,
    "editor.renderWhitespace": "none",
    "editor.renderControlCharacters": false,
    "workbench.iconTheme": "material-icon-theme",
    "workbench.colorTheme": "One Monokai",
    "terminal.integrated.shell.windows": "C:\\Windows\\System32\\cmd.exe",
    "files.autoSave": "off",
    "jshint.enable": false,
    "discord.detailsEditing": "Editando {filename}",
    "discord.detailsDebugging": "Editando {filename}",
    "discord.detailsIdle": "Inativo",
    "discord.largeImage": "Editando {LANG}",
    "discord.largeImageIdle": "Inativo",
    "discord.lowerDetailsDebugging": "Depurando: {workspace}",
    "discord.lowerDetailsEditing": "Área de Trabalho: {workspace}",
    "discord.lowerDetailsIdle": "Inativo",
    "discord.lowerDetailsNotFound": "Sem área de trabalho."
