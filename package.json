{
    "name": "infinityapp",
    "version": "0.01.5",
    "descrection": "Um app universal",
    "main": "app.js",
    "scripts": {
        "start": "electron . InfinityApp",
        "buildwin": "electron-packager . InfinityApp --version 1.7.6 --platform win32  --arch=x64 --icon ./img/logo.png --out ./dist --overwrite",
