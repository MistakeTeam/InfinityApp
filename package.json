{
	"author": "Mistake Team",
	"bin": {
		"electron": "./node_modules/.bin/electron"
	},
	"build": {
		"appId": "com.mistake.infinity",
		"productName": "Infinity",
		"asar": true,
		"copyright": "Copyright Â© 2018-2019 Mistake Team",
		"generateUpdatesFilesForAllChannels": true,
		"protocols": {
			"name": "infinityapp",
			"schemes": [
				"infinityapp"
			]
		},
		"files": [
			"electron/**/*",
			"!electron/installers",
			"!**/node_modules/*/{CHANGELOG.md,README.md,README,readme.md,readme,test,__tests__,tests,powered-test,example,examples,*.d.ts}",
			"!**/node_modules/.bin",
			"!**/*.{iml,o,hprof,orig,pyc,pyo,rbc,swp,csproj,sln,xproj}",
			"!**/._*",
			"!.editorconfig",
			"!**/{.DS_Store,.git,.hg,.svn,CVS,RCS,SCCS,__pycache__,thumbs.db,.gitignore,.gitattributes,.flowconfig,.yarn-metadata.json,.idea,.vs,appveyor.yml,.travis.yml,circle.yml,npm-debug.log,.nyc_output,yarn.lock,.yarn-integrity}"
		],
		"directories": {
			"output": "electron\\installers"
		},
		"win": {
			"target": [
				{
					"target": "nsis",
					"arch": [
						"x64"
					]
				}
			],
			"icon": "electron\\assets\\logo.ico"
		},
		"nsis": {
			"runAfterFinish": true,
			"oneClick": true,
			"artifactName": "InfinitySetup.${ext}"
		},
		"publish": [
			{
				"provider": "github",
				"owner": "MistakeTeam",
				"repo": "InfinityApp"
			}
		],
		"includePdb": false
	},
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
	],
	"dependencies": {
		"axios": "^0.18.0",
		"bcryptjs": "^2.4.3",
		"bluebird": "^3.5.4",
		"body-parser": "^1.19.0",
		"chalk": "^2.4.2",
		"connect-flash": "^0.1.1",
		"connect-mongo": "^2.0.3",
		"cookie-parser": "^1.4.4",
		"express": "^4.17.0",
		"express-session": "^1.16.1",
		"fluent-ffmpeg": "^2.1.2",
		"hls.js": "^0.12.4",
		"jsonwebtoken": "^8.5.1",
		"log-symbols": "^3.0.0",
		"moment": "^2.24.0",
		"moment-timezone": "^0.5.25",
		"mongoose": "^5.5.11",
		"mp4-stream": "^3.0.0",
		"numbro": "^2.1.2",
		"passport": "^0.4.0",
		"passport-local": "^1.0.0",
		"passport-local-mongoose": "^5.0.1",
		"react": "^16.8.6",
		"react-dom": "^16.8.6",
		"react-redux": "^7.0.3",
		"react-router-dom": "^5.0.0",
		"react-router-redux": "^4.0.8",
		"react-transition-group": "^4.0.1",
		"redux": "^4.0.1",
		"redux-router": "^2.1.2",
		"redux-thunk": "^2.3.0",
		"serve-favicon": "^2.5.0",
		"styled-components": "^4.2.0",
		"validator": "^11.0.0"
	},
	"devDependencies": {
		"@babel/core": "^7.4.5",
		"@babel/preset-env": "^7.4.5",
		"@babel/preset-es2015": "^7.0.0-beta.53",
		"@babel/preset-react": "^7.0.0",
		"@types/jquery": "^3.3.29",
		"babel-core": "^6.26.3",
		"babel-loader": "^8.0.6",
		"babel-register": "^6.26.0",
		"css-loader": "^2.1.1",
		"electron": "^5.0.2",
		"electron-builder": "^20.40.2",
		"electron-is": "^3.0.0",
		"electron-reload": "^1.4.0",
		"electron-updater": "^4.0.6",
		"i18n-webpack-plugin": "^1.0.0",
		"jsx-loader": "^0.13.2",
		"less": "^3.9.0",
		"less-loader": "^5.0.0",
		"mini-css-extract-plugin": "^0.6.0",
		"morgan": "^1.9.1",
		"style-loader": "^0.23.1",
		"webpack": "^4.32.2"
	},
	"devEngines": {
		"node": "^12.3.1"
	},
	"license": "GPL-3.0",
	"main": "bin/www",
	"name": "infinityapp",
	"repository": {
		"type": "git",
		"url": "git+ssh://git@github.com/MistakeTeam/InfinityApp.git"
	},
	"scripts": {
		"fast": "yarn run webpack-splash && yarn run webpack-app && yarn run start",
		"start": "node bin/www",
		"webpack-splash": "webpack-cli ./electron/splash/main.js -o ./electron/splash/index.js",
		"webpack-app": "webpack-cli --config webpack.config.js",
		"electron": "electron ./electron/index.js Infinity",
		"electron-setup": "yarn build.js"
	},
	"version": "v0.0.1"
}
